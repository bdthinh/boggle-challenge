{
  "name": "boggle",
  "main": "app.js",
  "version": "0.0.1",
  "description": "boggle",
  "scripts": {
    "start": "node ./app.js",
    "dev": "node ./cli/web.js",
    "debug": "node inspect ./cli/dev-server.js",
    "test": "NODE_ENV=test jest --logHeapUsage --runInBand",
    "build": "rimraf build && NODE_ENV=production webpack --display-error-details --progress --config ./cli/webpack.prod.js",
    "dev:dll": "rimraf .web && webpack --config ./cli/webpack.dev.dll.js",
    "postinstall": "npm run dev:dll",
    "postuninstall": "npm run dev:dll",
    "lint": "NODE_ENV=development eslint ./src --ext .js --ignore-path .gitignore --ignore-path .eslintignore",
    "lintfix": "NODE_ENV=development eslint ./src --ext .js --ignore-path .gitignore --ignore-path .eslintignore --cache --fix"
  },
  "author": "Thinh D. Bui",
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "testRegex": "((/__tests__/.*\\.(test|spec))\\.(js)$|app/react/.*\\-test\\.(js)$)",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "vendor"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/utils/fileMock.js",
      "^.+\\.(scss|css|less)(\\?\\w*)?$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.31",
    "@babel/core": "7.0.0-beta.31",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.31",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.31",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0-beta.31",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.31",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.31",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.31",
    "@babel/plugin-transform-react-jsx-self": "7.0.0-beta.31",
    "@babel/plugin-transform-react-jsx-source": "7.0.0-beta.31",
    "@babel/plugin-transform-runtime": "7.0.0-beta.31",
    "@babel/polyfill": "7.0.0-beta.31",
    "@babel/preset-env": "7.0.0-beta.31",
    "@babel/preset-react": "7.0.0-beta.31",
    "@babel/preset-stage-2": "7.0.0-beta.31",
    "@babel/register": "7.0.0-beta.31",
